#include<stdio.h>
#include<stdlib.h>
void merge(int* a, int m, int* b, int n)
{
	int i, j = 0,t;
	for (i = m; i < m + n; i++,j++)
		a[i] = b[j];
	for (i = 0; i < m + n - 1; i++)
	{
		for (j = 0; j < m + n - 1 - i; j++)
		{
			if (a[j] >= a[j + 1])
			{
				t = a[j];
				a[j] = a[j + 1];
				a[j + 1] = t;
			}
		}
	}
}
int main()
{
	int a[] = { 1,2,3,8,0,0 };
	int b[] = { 2,5,6 };
	merge(a, 4, b, 3);
	for (int i = 0; i < 7; i++)
		printf("%d", a[i]);
	system("pause");
	return 0;
}
